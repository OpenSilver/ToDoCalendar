<Application
    x:Class="ToDoCalendar.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sys="clr-namespace:System;assembly=mscorlib"
    xmlns:media="using:System.Windows.Media"
    xmlns:mt="http://opensilver.net/themes/modern">
    <Application.Theme>
        <mt:ModernTheme/>
    </Application.Theme>
    <Application.Resources>
        <ResourceDictionary>
            <SolidColorBrush x:Key="PhoneAccentBrush" Color="Purple"/>
            <SolidColorBrush x:Key="PhoneForegroundBrush" Color="Black"/>
            <SolidColorBrush x:Key="PhoneChromeBrush" Color="White"/>
            <sys:Double x:Key="PhoneFontSizeNormal">12</sys:Double>
            <FontFamily x:Key="PhoneFontFamilyNormal">ms-appx:///ToDoCalendar/Fonts/Inter-VariableFont_slnt,wght.ttf</FontFamily>
        </ResourceDictionary>
    </Application.Resources>
</Application>