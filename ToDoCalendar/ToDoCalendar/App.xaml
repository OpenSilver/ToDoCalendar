<Application x:Class="ToDoCalendar.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <Application.Resources>
        <ResourceDictionary>
            <SolidColorBrush x:Key="PhoneAccentBrush" Color="Purple"/>
            <SolidColorBrush x:Key="PhoneAccentBrushLowerOpacity" Color="Purple" Opacity="0.2"/>
            <SolidColorBrush x:Key="PhoneForegroundBrush" Color="Black"/>
            <SolidColorBrush x:Key="PhoneChromeBrush" Color="White"/>
            <sys:Double x:Key="PhoneFontSizeNormal">12</sys:Double>
            <FontFamily x:Key="PhoneFontFamilyNormal">ms-appx:///Fonts/Inter_VariableFont_slnt_wght.ttf</FontFamily>
        </ResourceDictionary>
    </Application.Resources>
</Application>
